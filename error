curl -X POST "http://38.147.83.31:8000/synthesize" \
  -H "Content-Type: application/json" \
  -d '{
    "text": "Hey, this is my chatterbox clone talking from Firebase!",
    "reference_url": "https://firebasestorage.googleapis.com/v0/b/the-course-of-the-future.firebasestorage.app/o/clones%2Foutput_merged.mp3?alt=media&token=7c2955a1-4de4-410a-8a18-39b0c8214fe7",
    "exaggeration": 0.5
  }' \
  --output output.wav
ssh -L 8000:localhost:8000 theeseus@38.147.83.31


